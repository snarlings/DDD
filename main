local Mana = loadstring(game:HttpGet("https://pastebin.com/raw/inP2PXxe"))()

local Win = Mana:Window("Dogs Do Drugs") -- Creates the Window AKA Adds the ScreenGui to CoreGui lol

local MainStuff = Mana:SName("MainStuff") -- Creates a littel information mark in the top left looks cool ngl try it

local Autofarm = Win:Tab("Misc") -- This Creates a Tab on the left Side

Autofarm:Toggle("Toggle1", false, function(t) -- This is A Toggle
hi = loadstring(game:HttpGet("https://pastebin.com/raw/7rXZ9VNc", true))()
   while hi do task.wait()
      print("Running Loop")
   end
end)

Autofarm:Button("R15 Animation Changer", function() -- This is A Button
loadstring(game:HttpGet("https://raw.githubusercontent.com/Sn8ow/Boblox-Fuck/main/scripts/Roblox%20Animation%20Script.lua", true))()
end)

Autofarm:Button("Anti Afk", function() -- This is A Button
game:GetService("Players").LocalPlayer.Idled:connect(function()
	game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),game:GetService("Workspace").CurrentCamera.CFrame)
	wait(1)
	game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),game:GetService("Workspace").CurrentCamera.CFrame)
	print("Prevented Kick(Idle) At " .. os.time())
end)
end)

Autofarm:Button("Fling Gui", function() -- This is A Button
